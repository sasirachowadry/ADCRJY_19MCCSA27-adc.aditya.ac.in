# ADCRJY_19MCCSA27-adc.aditya.ac.in
import java.util.*;
class sasi
 {
static Integer fact(int x)
{
  if(x==0||x==1)
  return Integer.One;
  else
  return Integer.valueOf(x).multiply(fact(x-1));
}
   public static void main(String args[])
   { 
     int a=0;
    Scanner s= new Scanner(System.in);
    int t= s.nextInt();
    
    for(int i=0;i<t;i++)
    {
    a=s.nextInt();
    System.out.println(fact(a));
    }
   }
 }

........................
OUTPUT
1
2
120
6
