# ADCRJY_19MCCSA27-adc.aditya.ac.in
import java.util.*;
class sasi
{
	static int main(int n)
	{
		if (n < 0) 
			return -1;
		int count = 0;
		for (int i = 5; n / i >= 1; i *= 5)
			count += n / i;
		return count;
    }
	public static void main(String[] args)
	{
		int n = 5;
		System.out.println("Count of trailing 0s in " + n+ "! is "	+ main(n));	
				
	}
}
..............................................
 Output:
Count of trailing 0s in 5! is 1
