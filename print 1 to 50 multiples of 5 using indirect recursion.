# ADCRJY_19MCCSA27-adc.aditya.ac.in
class sasi
{
	static final int N = 50;
	static int n = 1;
	static void fun1()
	{
		if (n <= N)
		{
			System.out.printf("%d ", n);
			n++;
			fun2();
		}
		else
		{
			return;
		}
	}
	static void fun2()
	{
		if (n <= N)
		{
			System.out.printf("%d ", n);
			n++;
			fun1();
		}
		else
		{
			return;
		}
	}
	public static void main(String[] args)
	{
		fun1();
	}
}
