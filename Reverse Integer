# ADCRJY_19MCCSA27-adc.aditya.ac.in
.public class main 
{
    public static void main(String args[])
    {
        int n=123456789;
        int rem, rev=0;
        while(n>0)
        {
            rem= n%10;
            rev=rev*10+rem;
            n=n/10;
        }
        System.out.println(rev);
    }
}

................................................
output:
 987654321


